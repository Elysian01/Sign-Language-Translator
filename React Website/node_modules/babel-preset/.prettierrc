{
  "arrowParens": "always",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "parser": "babel",
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json",
        "trailingComma": "none"
      }
    },
    {
      "files": ".babelrc",
      "options": {
        "parser": "json",
        "trailingComma": "none"
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript",
        "trailingComma": "none"
      }
    },
    {
      "files": "*.tsx",
      "options": {
        "parser": "typescript",
        "trailingComma": "none"
      }
    },
    {
      "files": "*.json",
      "options": {
        "parser": "json",
        "trailingComma": "none"
      }
    }
  ]
}
